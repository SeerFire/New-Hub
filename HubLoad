   Name = "Credits",
   Callback = function()
       Rayfield:Notify({
   Title = "Credits",
   Content = "Credits Goes To Rayfield & UnseenProfile",
   Duration = 6.5,
   Image = nil,
   Actions = { -- Notification Buttons

      Ignore = { -- Duplicate this table (or remove it) to add and remove buttons to the notification.
         Name = "Okay!",
         Callback = function()
            print("The user tapped Okay!")
         end
      },

},
})
   end,
})
